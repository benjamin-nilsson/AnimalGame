<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="450.0" prefWidth="768.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="animalgame.controllers.SaveGameController">
   <children>
      <TextField fx:id="gameName" layoutX="310.0" layoutY="213.0" promptText="Name your game" />
      <Button fx:id="saveButton" layoutX="418.0" layoutY="247.0" mnemonicParsing="false" onAction="#saveCurrentGame" style="-fx-background-color: gray;" text="Save" textFill="WHITE" />
      <Text layoutX="320.0" layoutY="41.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Save Game">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
